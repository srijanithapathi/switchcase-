#include<stdio.h>
int main()
{
	int food;
	printf("Enter a number between 1-5");
	scanf("%d",&food);
	switch(food)
    	{
		case 1:printf("Food item: Pizza \n price:239");
		break;
		case 2:printf("Food item: Burger \n price:129");
		break;
		case 3:printf("Food item: Pasta \n price:179");
		break;
		case 4:printf("Food item: French fries \n price:120");
		break;
		case 5:printf("Food item: Sandwich \n price:50");
	   	break;
		default:printf("Invalid choice");
		 
	    }
}
